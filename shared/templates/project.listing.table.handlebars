<!-- shared/table/project.listing.table.handlebars -->

<table class="table table-striped table-responsive table-hover table-clickable table-href pagination-projects">
    <thead>
    <tr>
        <td colspan="4" >{{> pagination}}</td>
    </tr>
    <tr>
        <th>#</th>
        <th>Namn</th>
        <th>Status</th>
        <th>AO Nummer</th>
    </tr>
    </thead>

    <tbody>
    {{#each entries}}
    <tr data-href='/projects/{{_id}}'>
        <th scope="row">{{tableIndex @index pagination=../pagination}}</th>
        <td>{{name}}</td>
        <td>{{status}}</td>
        <td>{{ao_number}}</td>
    </tr>
    {{/each}}
    </tbody>

    <tfoot>
    <tr>
        <td colspan="4">{{> pagination}}</td>
    </tr>
    </tfoot>
</table>