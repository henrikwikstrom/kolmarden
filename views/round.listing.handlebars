<!-- views/objects.listing.handlebars -->

<h3>Ronder</h3>
<hr>
<form method="GET" action="/rounds/new">
    <button class="btn btn-primary" >Ny rond</button>
</form>
<hr>

<table class="table table-striped table-responsive table-hover table-clickable table-href">
    <thead>
    <tr>
        <td colspan="4">{{> pagination }}</td>
    </tr>
    <tr>
        <th>#</th>
        <th>Namn</th>

    </tr>
    </thead>
    <tbody>
    {{#each entries}}
        <tr data-href='/rounds/{{_id}}'>
            <th scope="row">{{tableIndex @index pagination=../pagination}}</th>
            <td>{{name}}</td>

        </tr>
    {{/each}}
    </tbody>
    <tfoot>
    <tr>
        <td colspan="4">{{> pagination }}</td>
    </tr>
    </tfoot>
</table>